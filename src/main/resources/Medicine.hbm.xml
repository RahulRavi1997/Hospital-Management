<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
          "-//Hibernate/Hibernate Configuration DTD 5.3//EN"
          "http://hibernate.sourceforge.net/hibernate-configuration-5.3.dtd">


 <hibernate-mapping>
  <class name="com.ideas2it.hospitalmanagement.medicine.model.Medicine" table="medicine">
    <id name="id" column = "ID" type="int">
     <generator class="increment"/>   
    </id>
    <property name="name" column="NAME" type="string"></property>
    <property name="quantity" column="QUANTITY" type="double"></property>
    <property name="amount" column="AMOUNT" type="double"></property>
    <property name="active" column="ACTIVE" type="boolean"></property>
    
  </class>
 </hibernate-mapping>
