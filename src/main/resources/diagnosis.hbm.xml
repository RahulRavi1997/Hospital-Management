<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 5.3//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">
<hibernate-mapping>
	<class name="com.ideas2it.hospitalmanagement.diagnosis.model.Diagnosis" table="diagnosis">
		<id name="id" column="id">
			<generator class="increment"></generator>
		</id>
		<property name="date" column="DATE" type="date" />
		<property name="bloodGroup" column="BLOODGROUP" type="string"/>
		<property name="visitId" column="VISIT_ID" type="integer" />
		<property name="weight" column="WEIGHT" type="double" />
		<property name="height" column="HEIGHT" type="double" />
		<property name="bloodSugar" column="BLOOD_SUGAR" type="double" />
		<property name="bodyTemperature" column="BODY_TEMPERATURE" type="double" />
		</class>
</hibernate-mapping>
